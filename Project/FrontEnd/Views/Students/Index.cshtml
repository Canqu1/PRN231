@model IEnumerable<BackEnd.DTO.StudentRep>

<!DOCTYPE html>
<html>
<head>
    <title>Students</title>
</head>
<body>
    <h1>Students List</h1>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Details</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.StudentId</td>
                    <td>@student.Name</td>
                    <td>
                        <a href="@Url.Action("Details", "Students", new { id = student.StudentId })">View Details</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
